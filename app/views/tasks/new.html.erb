<h1>Criar uma nova tarefa</h1>

<%= form_with model: @task do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :details %>
  <%= form.text_field :details %>
  <%= form.label :completed, "Finished?" %>
  <%= form.check_box :completed %>
  <%= form.submit %>
<% end %>
